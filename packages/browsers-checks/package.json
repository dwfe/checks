{
  "name": "@dwfe/browsers-checks",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm run clean && react-scripts build",
    "start": "react-scripts start",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/node": "12",
    "@types/react": "16",
    "@types/react-dom": "16",
    "@types/autosize": "*",
    "react": "17",
    "react-dom": "17",
    "react-scripts": "4",
    "typescript": "4",
    "rimraf": "3",
    "classnames": "2"
  },
  "dependencies": {
    "@do-while-for-each/path-resolver": "2",
    "@do-while-for-each/browser-router": "2",
    "tsyringe": "4",
    "reflect-metadata": "0",
    "autosize": "4"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
